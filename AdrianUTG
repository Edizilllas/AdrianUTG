local function YOXUL_fake_script() -- _3Button.LocalScript 
	local script = Instance.new('LocalScript', _3Button)

	--Script made by sermi, edited by me, karaoke60000! (I added the right click function)
	--To open GUI, left click. To close, right click!
	function leftClick(mouse)
		 script.Parent.Parent.Main3.Visible = true --"NAMEHERE is where you put the name of the GUI"
	end
	script.Parent.MouseButton1Click:connect(leftClick)
	
	function rightClick(mouse)
		script.Parent.Parent.Main3.Visible = false
	end
	script.Parent.MouseButton2Click:connect(rightClick)
end
